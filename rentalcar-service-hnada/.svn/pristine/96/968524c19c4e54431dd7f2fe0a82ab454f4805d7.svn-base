<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
              http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
              http://www.springframework.org/schema/tx
              http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
              http://www.springframework.org/schema/context
              http://www.springframework.org/schema/context/spring-context-2.5.xsd">




	<bean id="ClientTransformer"
		class="com.unnamed.carrental.transformer.ClientTransformer" />

	<!-- Dao Beans -->
	<bean id="ClientDaoImpl"
		class="com.unnamed.carrental.servicedao.impldao.ClientDaoImpl" />
	<bean id="InsuranceDaoImpl"
		class="com.unnamed.carrental.servicedao.impldao.InsuranceDaoImpl" />
	<bean id="InsuranceTypeDaoImpl"
		class="com.unnamed.carrental.servicedao.impldao.InsuranceTypeDaoImpl" />


	<!-- Service Beans -->
	<bean id="ClientServiceImpl"
		class="com.unnamed.carrental.servicedao.impldao.ClientServiceImpl" />
	<bean id="InsuranceServiceImpl"
		class="com.unnamed.carrental.servicedao.impldao.InsuranceServiceImpl" />
	<bean id="InsuranceTypeServiceImpl"
		class="com.unnamed.carrental.servicedao.impldao.InsuranceTypeServiceImpl" />





</beans>